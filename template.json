{
  "index_patterns": [
    "result*"
  ],
  "mappings": {
    "dynamic_templates": [
      {
        "duration": {
          "match": "*duration",
          "mapping": {
            "type": "double"
          }
        }
      },
      {
        "timestamps": {
          "match": "*time",
          "mapping": {
            "type": "date",
            "format": "EEE LLL  d HH:mm:ss yyyy"
          }
        }
      },
      {
        "strings": {
          "match_mapping_type": "string",
          "match": "*",
          "mapping": {
            "type": "keyword"
          }
        }
      }
    ]
  }
}
